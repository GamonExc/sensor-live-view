{
  "expo": {
    "name": "Excello Ts App",
    "slug": "SensorApp",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/ts_app_icon.png",
    "scheme": "sensorapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.SensorApp"
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/ts_app_icon.png",
        "backgroundImage": "./assets/images/ts_app_icon.png",
        "monochromeImage": "./assets/images/ts_app_icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.SensorApp"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/excello-logo.png",
          "imageWidth": 180,
          "resizeMode": "contain",
          "backgroundColor": "#040000",
          "dark": {
            "backgroundColor": "#ffffff",
            "image": "./assets/images/excello-logo-black.png"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e4460db3-65d3-473f-9af5-a55e223d8438"
      }
    }
  }
}
